@base <https://celine-eu.github.io/ontologies/celine/> .
@prefix celine: <https://celine-eu.github.io/ontologies/celine#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

celine:Scenario a owl:Class ; rdfs:label "Scenario" .
celine:SimulationRun a owl:Class ; rdfs:label "Simulation Run" .

celine:hasScenario a owl:ObjectProperty ;
  rdfs:domain celine:EnergyCommunity ; rdfs:range celine:Scenario .

celine:executesScenario a owl:ObjectProperty ;
  rdfs:domain celine:SimulationRun ; rdfs:range celine:Scenario .

celine:capexEur a owl:DatatypeProperty ; rdfs:range xsd:float .
celine:annualSavingsEur a owl:DatatypeProperty ; rdfs:range xsd:float .
celine:simplePaybackYears a owl:DatatypeProperty ; rdfs:range xsd:float .
